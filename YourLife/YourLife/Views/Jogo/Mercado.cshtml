@{
    Layout = "~/Views/Jogo/Layout.cshmtl";
}

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

<html>
<body>
    <table>
        <thead>
            <tr>
                <th>Moradias</th>
                <th>Descrição</th>
                <th>Preço</th>
            </tr>
        </thead>
        

        <tbody>
            @{ int cont = 1;}

            @foreach (var mc in ViewBag.Mercado)
            {
                <tr>
                    <td>@mc.tipoProduto</td>
                    <td>@mc.produto</td>
                    <td>@mc.descricao</td>
                    <td>$@mc.preco</td>
                </tr>

                cont = cont + 1;

            }
        </tbody>
    </table>
</body>
</html>


