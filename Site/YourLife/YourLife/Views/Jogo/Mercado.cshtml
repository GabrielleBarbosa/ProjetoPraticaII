@{
    Layout = "~/Views/Shared/Layout.cshtml";
}

    <table class="highlight centered" style=" border: 1px solid #e0e0e0;">
        <thead>
            <tr>
                <th>Tipo</th>
                <th>Produto</th>
                <th>Descrição</th>
                <th>Preço</th>
            </tr>
        </thead>

        <tbody>

            @foreach (var mc in ViewBag.Mercado)
            {
                <tr>
                    <td>@mc.TipoProduto</td>
                    <td>@mc.Produto</td>
                    <td>@mc.Descricao</td>
                    <td>@mc.Preco</td>
                </tr>
            }
        </tbody>
    </table>